{
  "name": "swagger2aglio",
  "description": "Swagger API Documentation HTML Generator",
  "main": "index.js",
  "bin": {
    "swagger2aglio": "./index.js"
  },
  "scripts": {
    "cover": "istanbul cover node_modules/mocha/bin/_mocha",
    "coveralls": "npm run cover -- --report lcovonly && cat ./coverage/lcov.info | coveralls",
    "examples": "set NOCACHE=1&& node examples.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "start": "set NOCACHE=1&& node server.js",
    "test": "eslint index.js test/test.js && mocha"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/psastras/swagger2aglio.git"
  },
  "keywords": [
    "swagger",
    "aglio",
    "api",
    "apiblueprints",
    "blueprints",
    "documentation"
  ],
  "author": "Paul Sastrasinh",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/psastras/swagger2aglio/issues"
  },
  "homepage": "https://github.com/psastras/swagger2aglio#readme",
  "dependencies": {
    "aglio": "^2.3.0",
    "colors": "^1.1.2",
    "commander": "^2.9.0",
    "html-minifier": "^3.5.0",
    "json-schema-ref-parser": "^3.1.2",
    "prettyjson": "^1.1.3",
    "swag2blue": "^1.0.1"
  },
  "devDependencies": {
    "coveralls": "^2.11.9",
    "eslint": "^3.0.1",
    "express": "^4.14.0",
    "istanbul": "^0.4.4",
    "mocha": "^3.2.0",
    "mocha-lcov-reporter": "^1.2.0",
    "morgan": "^1.7.0",
    "reload": "^1.0.0",
    "semantic-release": "^6.3.2",
    "serve-index": "^1.8.0",
    "serve-static": "^1.11.1",
    "watch": "^1.0.2"
  }
}
